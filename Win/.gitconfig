[user]
	name  = KoynovStas
	email = skojnov@yandex.ru

[core]
	excludesfile = ~/.gitignore_global
	editor       = ~/npp.sh
	quotepath    = false
	autocrlf     = true
	askpass      =

[init]
	defaultBranch = master

[color]
	ui = true

[alias]
	ignore  = "!gi() { curl -L -s https://www.gitignore.io/api/$@ ;}; gi"
	rclone  = clone --recurse-submodules

[credential "helperselector"]
	selected = <no helper>

[credential]
	helper = cache